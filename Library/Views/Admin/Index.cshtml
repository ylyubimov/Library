
@model Library.Models.Admin
@{
    ViewBag.Title = "Админ Админович";
}

<h2 align="center">Our Admin:</h2>
@{Html.BeginForm();}
<fieldset>
    <div align="center">
        <label>Имя</label>
        @Html.TextBoxFor(model => model.UserName)
    </div>
    <div align="center">
        <label>Пароль</label>
        @Html.TextBoxFor( model => model.PasswordHash)
    </div>
</fieldset>
@{Html.EndForm();}


